    "# Validation functions\n",
    "def validate_smiles(smiles):\n",
    "    \"\"\"Validate a SMILES string by attempting to create an RDKit molecule\"\"\"\n",
    "    if not smiles or smiles.strip() == \"\":\n",
    "        return True  # Empty is ok\n",
    "    \n",
    "    try:\n",
    "        mol = Chem.MolFromSmiles(smiles)\n",
    "        return mol is not None\n",
    "    except:\n",
    "        return False\n",
    "\n",
    "def validate_uniprot(uniprot_id):\n",
    "    \"\"\"Validate a UniProt ID using a regex pattern and online check\"\"\"\n",
    "    if not uniprot_id or uniprot_id.strip() == \"\":\n",
    "        return True  # Empty is ok\n",
    "    \n",
    "    # Basic pattern check for UniProt IDs\n",
    "    pattern = re.compile(r'^[A-Z0-9]{6,10}$')\n",
    "    if not pattern.match(uniprot_id):\n",
    "        return False\n",
    "    \n",
    "    # For a more thorough check, we could query the UniProt API\n",
    "    # This is commented out to avoid network requests during validation\n",
    "    # url = f\"https://www.uniprot.org/uniprot/{uniprot_id}.txt\"\n",
    "    # response = requests.get(url)\n",
    "    # return response.status_code == 200\n",
    "    \n",
    "    return True\n",
    "\n",
    "def validate_pdb(pdb_id):\n",
    "    \"\"\"Validate a PDB ID using a regex pattern\"\"\"\n",
    "    if not pdb_id or pdb_id.strip() == \"\":\n",
    "        return True  # Empty is ok\n",
    "    \n",
    "    # Basic pattern check for PDB IDs: 4 characters, alphanumeric\n",
    "    pattern = re.compile(r'^[A-Za-z0-9]{4}$')\n",
    "    return pattern.match(pdb_id) is not None\n",
    "\n",
    "# Function to create a styled header\n",
    "def styled_header(text, level=2):\n",
    "    \"\"\"Create a styled header for the interface\"\"\"\n",
    "    if level == 1:\n",
    "        color = \"#4285F4\"  # Blue\n",
    "        size = \"24px\"\n",
    "    elif level == 2:\n",
    "        color = \"#EA4335\"  # Red\n",
    "        size = \"20px\"\n",
    "    else:\n",
    "        color = \"#34A853\"  # Green\n",
    "        size = \"18px\"\n",
    "        \n",
    "    return HTML(f\"<h{level} style='color:{color}; font-size:{size}'>{text}</h{level}>\")"
   ]
